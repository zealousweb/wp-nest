{
	"name": "wpnest",
	"version": "1.0.2",
	"engines": {
		"node": ">=20"
	},
	"private": true,
	"description": "A starter theme for WordPress with modern JavaScript and CSS tooling.",
	"type": "module",
	"scripts": {
		"watch": "webpack --mode development --watch",
		"build": "webpack --mode production",
		"lint:js": "eslint -c config/config.eslint.js sources/**/*.js",
		"lint:js:fix": "eslint -c config/config.eslint.js sources/**/*.js --fix",
		"lint:scss": "stylelint -c config/config.stylelint.js sources/**/*.scss",
		"lint:scss:fix": "stylelint -c config/config.stylelint.js sources/**/*.scss --fix",
		"lint": "npm run lint:js && npm run lint:scss",
		"lint:fix": "npm run lint:js:fix && npm run lint:scss:fix",
		"phpcs": "phpcs",
		"phpcbf": "phpcbf",
		"prepare": "husky install"
	},
	"keywords": [
		"WordPress",
		"theme",
		"starter",
		"webpack",
		"sass"
	],
	"author": "zwt",
	"license": "ISC",
	"devDependencies": {
		"@babel/core": "^7.28.6",
		"@babel/preset-env": "^7.28.6",
		"@eslint/js": "^9.39.2",
		"@types/jquery": "^3.5.33",
		"autoprefixer": "^10.4.23",
		"babel-loader": "^10.0.0",
		"cspell": "^9.6.2",
		"css-loader": "^7.1.2",
		"css-minimizer-webpack-plugin": "^7.0.4",
		"eslint": "^9.39.2",
		"glob": "^13.0.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"mini-css-extract-plugin": "^2.10.0",
		"postcss": "^8.5.6",
		"postcss-loader": "^8.2.0",
		"purgecss-webpack-plugin": "^8.0.0",
		"sass": "^1.97.3",
		"sass-loader": "^16.0.6",
		"stylelint": "^17.0.0",
		"stylelint-config-clean-order": "^8.0.0",
		"stylelint-config-standard-scss": "^17.0.0",
		"terser-webpack-plugin": "^5.3.16",
		"webpack": "^5.104.1",
		"webpack-build-notifier": "^3.1.0",
		"webpack-cli": "^6.0.1",
		"webpack-manifest-plugin": "^6.0.1",
		"webpackbar": "^7.0.0"
	},
	"dependencies": {
		"@fancyapps/ui": "^6.1.9",
		"fs": "^0.0.1-security",
		"jquery": "^4.0.0",
		"swiper": "^12.0.3"
	},
	"lint-staged": {
		"sources/**/*.js": [
			"eslint -c config/config.eslint.js --fix"
		],
		"sources/**/*.scss": [
			"stylelint -c config/config.stylelint.js --fix"
		]
	}
}
